@using Kentico.PageBuilder.Web.Mvc
@using Kentico.Membership.Web.Mvc
@using Kentico.Content.Web.Mvc
@using GulfCoastWorkforceSolutions.Models.Widgets

<!DOCTYPE html>
<html>
<head id="head">
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta charset="UTF-8" />
    @Html.Kentico().PageDescription()
    @Html.Kentico().PageKeywords()
    @Html.Kentico().PageTitle(ViewBag.Title as string)
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=DM+Sans:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Inter:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
          rel="stylesheet">

    <link href="~/Content/css/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/css/animate.css" rel="stylesheet">
    <link href="~/Content/css/custom.css" rel="stylesheet">
    <link href="~/Content/css/responsive.css" rel="stylesheet">
    <link rel="canonical" href="@Url.Kentico().PageCanonicalUrl()" />
    @RenderSection("styles", required: false)
</head>
<body>
    
        <header class="main_header" role="banner">
            <div class="top_header">
                <div class="container">
                    <div class="top_header_cont">
                        <div class="top_menu">
                            <ul class="top_inner_menu">
                                <li> <a href="#">Calendar </a> </li>
                                <li> <a href="#"> News</a> </li>
                                <li> <a href="#">Childcare Financial Assistance </a> </li>
                            </ul>
                            <div class="select_language">
                                <select class="form_control">
                                    <option>Translate</option>
                                    <option>English</option>
                                    <option>Spain</option>
                                </select>
                            </div>
                            <div class="top_search">
                                <img src="img/search.png" style="max-height: 35px;width: 180px;" alt="">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <nav class="navbar navbar-expand-xl" aria-label="Top Navigation">
                <div class="container">
                    <div class="head_wrap">
                        <div class="logo_wrap">
                            <a class="navbar-brand" href="#">
                                <img src="img/logo.svg" alt=" Logo">
                            </a>
                        </div>

                        <div class="collapse navbar-collapse" id="navbarResponsive">
                            <nav class="nav_bottom" aria-label="Main Navigation">
                                <ul class="navbar-nav ">
                                    <li class="nav-item ">
                                        <a class="nav-link" href="#">Employers</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#" aria-haspopup="true" aria-expanded="false">Job Seekers</a>
                                        <ul class="dropdown-menu" aria-label="">
                                            <li><a href="#">lorem </a> </li>
                                            <li>
                                                <a href="#">lorem ipsum</a>
                                            </li>
                                            <li>
                                                <a href="#"> Lorem ipsum</a>
                                            </li>

                                        </ul>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#">Education</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#">Regional Economic Data</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#">About Us</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#">News</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="nav-link" href="#">Calendar</a>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="icon-bar top"></span>
                        <span class="icon-bar midlde"></span>
                        <span class="icon-bar last"></span>
                    </button>
                </div>
            </nav>
        </header>
        <main id="page_content">
            @RenderBody()
        </main>
        <footer class="footer ">
            <div class="container">
                <div class="row footer_row align-items-start">
                    <div class="col-md-4  foot_col ">
                        <div class="footer-logo">
                            <a href="#">
                                <img src="img/footer-logo.svg" alt="GCWS Logo" height="">
                            </a>
                        </div>
                        <ul class="footer-social d-flex list-unstyled mb-0">
                            <li>
                                <a href="#" class="footer-social-icon"><img src="img/facebook.svg" alt="Facebook"></a>
                            </li>
                            <li>
                                <a href="#" class="footer-social-icon"><img src="img/TwitterX.svg" alt="Twitter"></a>
                            </li>
                            <li>
                                <a href="#" class="footer-social-icon"><img src="img/instagram.svg" alt="Instagram"></a>
                            </li>
                            <li>
                                <a href="#" class="footer-social-icon"><img src="img/youtube.svg" alt="YouTube"></a>
                            </li>
                            <li>
                                <a href="#" class="footer-social-icon"><img src="img/Linked.svg" alt="LinkedIn"></a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-md-4 foot_col ">
